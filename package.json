{
  "name": "retile",
  "version": "0.0.0",
  "license": "MIT",
  "main": "lib/index.js",
  "bin": "lib/cli.js",
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "css-layout": "^1.1.1",
    "easyimage": "^2.1.0",
    "interpolate": "^0.1.0",
    "invariant": "^2.2.1",
    "lodash.merge": "^4.6.0",
    "meow": "^3.7.0",
    "mkdirp": "^0.5.1",
    "percent": "^1.1.1",
    "probe-image-size": "^2.1.1"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.14.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2017": "^6.14.0",
    "babel-register": "^6.14.0",
    "babel-watch": "^2.0.2",
    "cross-env": "^2.0.1",
    "xo": "^0.16.0"
  },
  "scripts": {
    "test": "ava",
    "lint": "xo",
    "build": "babel src --out-dir lib"
  },
  "babel": {
    "presets": [
      "es2015",
      "es2017"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "esnext": true,
    "rules": {
      "xo/no-process-exit": ["off"],
      "babel/object-curly-spacing": ["error", "always"]
    }
  },
  "files": ["lib"]
}
